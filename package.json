{
  "name": "eslint-config-space-single",
  "version": "0.1.1",
  "description": "Sharable ESLint config, based on xo-space, but with single quotes + some additional settings.",
  "keywords": [
    "eslint",
    "xo",
    "xo-space"
  ],
  "license": "MIT",
  "author": {
    "name": "Stefan Walther",
    "url": "https://github.com/stefanwalther",
    "twitter": "waltherstefan"
  },
  "files": [
    "index.js",
    "esnext.js",
    "browser.js"
  ],
  "main": "index.js",
  "repository": "https://github.com/stefanwalther/eslint-config-space-single",
  "scripts": {
    "docs": "verb",
    "test": "mocha './test/**/*.spec.js' --require './test/mocha.conf.js'",
    "lint:src": "eslint src",
    "test-coverage": "istanbul cover node_modules/.bin/_mocha  --require ./test/mocha.conf.js -- -- -u exports -R spec test/**/* && codecov"
  },
  "dependencies": {
    "eslint-config-xo-space": "^0.16.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "codecov": "^2.1.0",
    "eslint": "^3.13.1",
    "is-plain-obj": "^1.1.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "temp-write": "^3.2.0"
  },
  "verb": {
    "run": true,
    "toc": false,
    "layout": "empty",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "eslint",
        "eslint-config-xo-space"
      ]
    },
    "lint": {
      "reflinks": true
    },
    "reflinks": [
      "verb",
      "verb-readme-generator"
    ]
  }
}
